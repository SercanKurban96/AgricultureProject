@using AgricultureProject.EntityLayer.Concrete
@model List<Address>

<h3 class="tittle">Bize Ulaşın</h3>
<p class="heading-btm-w3ls">Aşağıdaki form üzerinden bize mail gönderebilir veya telefonla arayabilirsiniz.</p>
@foreach (var item in Model)
{
    <div class="col-md-3 contact-left">
        <h4>Adresimiz</h4>
        <p>@item.AddressDescription1,</p>
        <p>@item.AddressDescription2,</p>
        <p>@item.AddressDescription3</p>
        <h4>Telefon Numaramız</h4>
        <p>@item.AddressDescription4</p>
    </div>
}